{
  "name": "devtinder-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run kill-port && vite",
    "dev:unsafe": "vite",
    "kill-port": "for /f \"tokens=5\" %a in ('netstat -aon ^| find \":5173\"') do taskkill /f /pid %a 2>nul || echo Port 5173 is free",
